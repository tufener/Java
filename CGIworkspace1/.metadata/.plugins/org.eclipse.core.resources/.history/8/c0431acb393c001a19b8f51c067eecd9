package model;

public abstract class Article {
	private String nom; 
	private int prix;
	private Ligne ligne; 
	
	
	public Article(String nom, int prix) {
		this.nom = nom;
		this.prix = prix;
	}
	
	


	public Article(String nom, int prix, Ligne ligne) {
		this.nom = nom;
		this.prix = prix;
		this.ligne = ligne;
	}




	@Override
	public String toString() {
		return "nom=" + nom + ", prix=" + prix +  "ligne=" + ligne;
	}
	
	
	
	
	

}
