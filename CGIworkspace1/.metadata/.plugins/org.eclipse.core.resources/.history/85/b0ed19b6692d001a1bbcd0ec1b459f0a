package projetTableau;

public class PorjetTableau {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		testIntv2();

	}
	
	static void testIntv1(){
		int[] tab = new int[3];
		System.out.println(tab[0]);
		tab[0]= 10;
		System.out.println(tab[0]);
		
		
	}
	
	static void testIntv2(){
		int[] tab = new int[3];
		tab[0]=10;
		tab[2]=100;
		for(int i=0;i<tab.length;i++) // tab.length permet de déterminer automatiquement la taille du tableau = pas besoin de manipuler des chiffres en dur
			System.out.println(tab[i]);

		
		
	}
	

}
