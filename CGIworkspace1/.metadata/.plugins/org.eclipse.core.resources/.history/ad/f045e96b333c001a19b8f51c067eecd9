package model;

public class Ligne {
	private Article article;
	private Caddie caddie;
	private int quantite;
	
	public Ligne(model.Article article, Caddie caddie, int quantite, String article2) {
		this.article = article;
		this.caddie = caddie;
		this.quantite = quantite;
		Article = article2;
	}

	@Override
	public String toString() {
		return "Ligne [article=" + article + ", caddie=" + caddie + ", quantite=" + quantite + ", Article=" + Article
				+ "]";
	}

	public int getQuantite() {
		return quantite;
	}

	public void setQuantite(int quantite) {
		this.quantite = quantite;
	}

	public Article getArticle() {
		return article;
	}


	
	
	
	
	
	

}
