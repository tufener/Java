package model;

public class Gateau extends Article {
	private EGateau egateau;

	public Gateau(String nom, int prix, EGateau egateau) {
		super(nom, prix);
		this.egateau = egateau;
	}

	@Override
	public String toString() {
		return "Gateau: " + egateau.name() + "Volume: " + egateau.getPrix();
	}

}
