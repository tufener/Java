package essaieHasret;

import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	static void testTableauInverse(){
		Scanner sc = new Scanner(System.in);
		System.out.println("Veuillez saisir une phrase dans la console ");
		String separation = " ";
		String str = sc.nextLine();
		String[] tab = str.split(separation);
		for(String s : tab)
			System.out.println(s);
	}

}
